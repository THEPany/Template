{
    "name": "thepany/template",
    "description": "Convert word templates to pdf",
    "license": "MIT",
    "authors": [
        {
            "name": "Cristian Gomez",
            "email": "cristiangomeze@hotmail.com"
        }
    ],
    "homepage": "https://github.com/thepany/template",
    "keywords": ["Laravel", "Template"],
    "require": {
        "illuminate/support": "^8.0",
        "phpoffice/phpword": "^0.17.0",
        "symfony/process": "^5.0",
        "kwn/number-to-words": "^1.11"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "orchestra/testbench": "^6.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Thepany\\Template\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Thepany\\Template\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Thepany\\Template\\TemplateServiceProvider"
            ],
            "aliases": {
                "Template": "Thepany\\Template\\Facades\\Template"
            }
        }
    }
}
